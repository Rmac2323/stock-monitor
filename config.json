{
    "stocks": [
        "AAPL",
        "GOOGL",
        "MSFT",
        "SMXT"
    ],
    "update_interval": 5,
    "timeframes": {
        "price": "15m",
        "rsi": {
            "interval": "15m",
            "period": 14
        },
        "sma": {
            "interval": "1d",
            "period": 20
        },
        "vwap": "15m",
        "support_resistance": {
            "interval": "1d",
            "lookback": 20
        },
        "trend": {
            "interval": "30m",
            "period": 20
        },
        "patterns": "15m"
    },
    "logging": {
        "enabled": true,
        "directory": "data"
    },
    "market_hours": {
        "timezone": "America/New_York",
        "open_time": "09:30",
        "close_time": "16:00",
        "monitor_outside_hours": false
    },
    "news": {
        "enabled": true,
        "update_interval": 3600,
        "newsapi_key": null,
        "sources": [
            "yahoo",
            "google",
            "newsapi"
        ]
    },
    "edgar": {
        "enabled": true,
        "update_interval": 1800,
        "url": "https://app.askedgar.io/gainers"
    }
}